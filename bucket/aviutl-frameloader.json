{
  "version": "0.01",
  "homepage": "http://spring-fragrance.mints.ne.jp/aviutl/",
  
  "depends": "aviutl",

  "url": "http://spring-fragrance.mints.ne.jp/aviutl/frameloader.zip",

  "post_install": [
    "cp \"$dir\\*\" \"$(appdir aviutl)\\current\""
  ],

  "notes": [
    "aviutl-frameloaderはアンインストールに対応してません",
    "scoop\\apps\\aviutl\\current から手動で削除してください"
  ]
}
